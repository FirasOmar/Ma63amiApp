<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.awj.ma63amiapp_admin.viewOrder"
    android:background="#000000">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/AdmintextView39"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Item Name"
        android:id="@+id/AdmintextView40"
        android:layout_below="@+id/AdmintextView39"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Item Price"
        android:id="@+id/AdmintextView41"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/AdmintextView40"
        android:layout_alignLeft="@+id/AdmintextView39"
        android:layout_alignStart="@+id/AdmintextView39" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Quantity"
        android:id="@+id/AdmintextView42"
        android:textColor="#ffffff"
        android:layout_toRightOf="@+id/AdmintextView39"
        android:layout_alignTop="@+id/AdmintextView41" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/AdmintextView43"
        android:layout_below="@+id/AdmintextView40"
        android:layout_alignLeft="@+id/AdmintextView40"
        android:layout_alignStart="@+id/AdmintextView40"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/AdmintextView67"
        android:textColor="#0000ff"
        android:layout_toLeftOf="@+id/AdmintextView67" />

    <TextView
        android:layout_width="35dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/AdmintextView67"
        android:textColor="#DAA520"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/AdmintextView41"
        android:gravity="center_horizontal" />

    <TextView
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/AdmintextView75"
        android:textColor="#00ff00"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/AdmintextView42"
        android:layout_alignRight="@+id/AdmintextView42"
        android:layout_alignEnd="@+id/AdmintextView42"
        android:layout_alignLeft="@+id/AdmintextView42"
        android:layout_alignStart="@+id/AdmintextView42"
        android:gravity="center_horizontal" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/Adminvieworderback"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/simpleback2" />

</RelativeLayout>
