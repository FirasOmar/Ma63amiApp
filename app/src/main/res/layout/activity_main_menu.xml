<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.example.myapplication.MainMenu"
    android:background="#eaeaea">

    <TableRow
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:id="@+id/tr1"
        android:background="#ffffff"
        android:onClick="back1_onclick">

        <ImageButton
            android:layout_height="60dp"
            android:layout_width="70dp"
            android:id="@+id/back"
            android:src="@drawable/back"
            android:onClick="back1_onclick"
            android:background="#ffffff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Main Menu"
            android:id="@+id/textView4"
            android:layout_marginTop="12dp"
            android:textSize="30dp" />
    </TableRow>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginTop="20dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:onClick="menu_onclick"
                android:src="@drawable/menu"
                android:background="#eaeaea"
                android:layout_weight="1" />

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/gal"
                android:layout_marginLeft="0dp"
                android:onClick="gallery_onclick"
                android:background="#eaeaea"
                android:layout_weight="1" />

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/gps"
                android:layout_marginLeft="0dp"
                android:background="#eaeaea"
                android:layout_weight="1"
                android:onClick="gps_onclick"/>
            </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Menu"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                android:gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Gallery"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                android:paddingLeft="@android:dimen/app_icon_size" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="GPS"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                android:paddingLeft="@android:dimen/app_icon_size" />
            </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp">

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/offers"
                android:onClick="offers_onclick"
                android:layout_weight="1"
                android:background="#eaeaea" />

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/reserve"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                android:background="#eaeaea"
                android:onClick="restable_onclick"/>

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/facebook"
                android:layout_marginLeft="0dp"
                android:onClick="fb_onclick"
                android:layout_weight="1"
                android:background="#eaeaea" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Offers"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                android:paddingLeft="28dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Reserve\n  Table"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                android:paddingLeft="45dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="FB Page"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                android:paddingLeft="35dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp">

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/info"
                android:onClick="about_rest_onclick"
                android:layout_weight="1"
                android:background="#eaeaea" />

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/account"
                android:layout_marginLeft="0dp"
                android:onClick="myacct_onclick"
                android:layout_weight="1"
                android:background="#eaeaea"
                />

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/evaluation"
                android:layout_marginLeft="0dp"
                android:onClick="evaluation_onclick"
                android:layout_weight="1"
                android:background="#eaeaea" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="    About\nRestaurant"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:paddingLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="My Account"
                android:layout_marginLeft="38dp"
                android:layout_weight="1" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Evaluation"
                android:layout_marginLeft="35dp"
                android:layout_weight="1" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp">

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/bill"
                android:layout_marginLeft="0dp"
                android:onClick="bill_onclick"
                android:layout_weight="1"
                android:background="#eaeaea" />

            <ImageView
                android:layout_width="72dp"
                android:layout_height="72dp"
                android:src="@drawable/order"
                android:layout_marginLeft="0dp"
                android:onClick="order_onclick"
                android:layout_weight="1"
                android:background="#eaeaea" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bill"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                android:paddingLeft="70dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Order"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                android:paddingLeft="78dp" />

        </TableRow>

    </TableLayout>

    <WebView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/webView2"
        android:background="#eaeaea"
        android:visibility="invisible" />


</LinearLayout>
